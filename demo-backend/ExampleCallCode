import java.util.Scanner;

class Rextester {
    public static void main(String[] args) {
        int count = new Scanner(System.in).nextInt();

        int n1 = 0, n2 = 1, n3 = 0;

        for (int i = 2; i <= count; ++i)
        {
            n3 = n1 + n2;
            n1 = n2;
            n2 = n3;
        }

        System.out.println(n3);
    }
}
